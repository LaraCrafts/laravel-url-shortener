{
    "name": "laracrafts/laravel-url-shortener",
    "description": "Powerful URL shortening tools in Laravel",
    "keywords": [
        "laravel",
        "lumen",
        "url",
        "shortening",
        "shortener",
        "bit.ly",
        "tinyURL",
        "shorte.st"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Choraimy Kroonstuiver",
            "email": "choraimy@live.nl"
        },
        {
            "name": "László Görög",
            "email": "contact@nergal.xyz"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.5 || ^6.0",
        "php-http/discovery": "^1.6",
        "psr/http-client-implementation": "^1.0",
        "psr/http-factory-implementation": "^1.0",
        "psr/http-message-implementation": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "illuminate/routing": "^5.1 || ^6.0",
        "mockery/mockery": "^1.2",
        "php-http/mock-client": "dev-upgrade-version-2",
        "phpunit/phpunit": "^5.2 || ^6.0 || ^7.0",
        "zendframework/zend-diactoros": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaraCrafts\\UrlShortener\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraCrafts\\UrlShortener\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "tests": "./vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\LaraCrafts\\UrlShortener\\UrlShortenerServiceProvider"
            ]
        }
    }
}
