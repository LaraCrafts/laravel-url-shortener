{
    "name": "laracrafts/laravel-url-shortener",
    "description": "Powerful URL shortening tools in Laravel",
    "keywords": [
        "laravel",
        "lumen",
        "url",
        "shortening",
        "shortener",
        "bit.ly",
        "tinyURL",
        "shorte.st"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Choraimy Kroonstuiver",
            "email": "choraimy@live.nl"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/promises": "^1.0",
        "guzzlehttp/psr7": "^1.4",
        "illuminate/support": "^5.1,<5.9",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.1",
        "phpunit/phpunit": "^5.2 || ^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "LaraCrafts\\UrlShortener\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraCrafts\\UrlShortener\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "tests": "./vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "aliases": {
                "UrlShortener": "\\LaraCrafts\\UrlShortener\\Support\\Facades\\UrlShortener"
            },
            "providers": [
                "\\LaraCrafts\\UrlShortener\\UrlShortenerServiceProvider"
            ]
        }
    }
}
